os: mac
and browser.host: chat.openai.com
-

chat new : 
    #Open a new chat
    key(cmd-shift-o)

chat focus:
    # Move to the input box  Of Chat GPT
    key(shift-esc)
    
chat delete:
    # Delete the current chat Not
    key(cmd-shift-del)

response copy:
    #copy the last reply    
    key(cmd-shift-c)

instruction edit:
    #edit the system prompt
    key(cmd-shift-i)

sidebar toggle:
    # show or hide the sidebar
    key(cmd-shift-s)
